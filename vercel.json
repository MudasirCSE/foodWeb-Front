{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/login",
      "dest": "/"
    },
    {
      "src": "/signup",
      "dest": "/"
    },
    {
      "src": "/cart",
      "dest": "/"
    },
    {
      "src": "/checkout",
      "dest": "/"
    },
    {
      "src": "/myorders",
      "dest": "/"
    },
    {
      "src": "/allorders",
      "dest": "/"
    }
  ]
}
